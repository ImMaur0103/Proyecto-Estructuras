<table class="table" cellpadding="0" cellspacing="0">
    <thead class="table-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DPI_CUI)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellido)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Municipio_residencia)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Departamento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Edad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prioridad)
            </th>
        </tr>
    </thead>
    @if (Model != null)
    {

        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.DPI_CUI)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nombre)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Apellido)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Municipio_residencia)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Departamento)
                    </td>
                    <th>
                        @Html.DisplayFor(modelItem => item.Edad)
                    </th>
                    <th>
                        @Html.DisplayFor(modelItem => item.Prioridad)
                    </th>
                </tr>
            }
        </tbody>
    }
</table>
