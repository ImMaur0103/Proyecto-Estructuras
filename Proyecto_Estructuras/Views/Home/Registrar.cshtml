    <form method="post" action="Registrar" class="Registrar">

        <div class="form-floating mb-3">
            <label for="floatingInput">Nombre</label>
            <input type="text" class="form-control" name="Nombre" placeholder="Nombre">
        </div>
        <div class="form-floating">
            <label for="floatingInput">Apellido</label>
            <input type="text" class="form-control" name="Apellido" placeholder="Apellido">
        </div>
        <div class="form-floating">
            <label for="floatingInput">DPI, o en caso de no poseer ingrese CUI</label>
            <input type="tel" class="form-control" name="DPI_CUI" placeholder="DPI/CUI">
        </div>
        <div class="form-floating mb-3">
            <label for="floatingInput">Departamento</label>
            <input type="text" class="form-control" name="Departamento" placeholder="Departamento">
        </div>
        <div class="form-floating">
            <label for="floatingInput">Municipo donde usted recide</label>
            <input type="text" class="form-control" name="Municipio_residencia" placeholder="Municipio de residencia">
        </div>
        <div class="form-floating">
            <label for="floatingInput">Edad</label>
            <input type="tel" class="form-control" name="Edad" placeholder="Edad">
        </div>
        <div class="form-floating">
            <label for="floatingInput">Vacunado</label>
            <input class="Checkbox" data-val="true" data-val-required="The Vacunado field is required." id="Vacunado" name="Vacunado" type="checkbox" value="true">
        </div>
        <div>
            <center><button type="submit" class="Boton">Agregar</button></center>
        </div>
    </form>
